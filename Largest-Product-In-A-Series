
myNum = ""

s=1
for i in range(0,13):
    s*=int(myNum[i])
print(f"s = {s}")    
f=0
l=12
res = 0
for i in range(13,len(myNum)):
    if int(myNum[i-13]) == 0:
        s=1
        for j in range(i-12,i+1):
            s*= int(myNum[j])
    else:
        s = s * int(myNum[i]) // int(myNum[i-13])
    print(f"myNum[{i}] = {myNum[i]}, myNum[{i-13}] = {myNum[i-13]}, i = {i}, s = {s}")
    if res < s:
        res = s;
        f=i-12
        l=i

print(f"f = {f}, l = {l}, res = {res}")


